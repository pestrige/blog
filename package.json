{
	"name": "production-project",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start:dev": "concurrently \"webpack serve\" \"node ./json-server/index.js\"",
		"start:dev-vite": "concurrently \"vite\" \"node ./json-server/index.js\"",
		"build:prod": "webpack --env mode=production apiUrl=https://blog-server-pestrige.vercel.app",
		"build:dev": "webpack --env mode=development",
		"lint": "eslint '**/*.{ts,tsx}'",
		"lint:prod": "NODE_ENV=production eslint '**/*.{ts,tsx}'",
		"lint:fix": "eslint '**/*.{ts,tsx}' --fix",
		"lint:scss": "npx stylelint \"**/*.scss\"",
		"lint:scss-fix": "npx stylelint \"**/*.scss\" --fix",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:ui": "npx loki test",
		"test:ui-approve": "npx loki approve",
		"test:ui-report": "node scripts/generateVisualJsonReport.js && reg-cli --from .loki/report.json --report .loki/report.html",
		"test:ui-ci": "npx loki --requireReference --reactUri file:./storybook-static",
		"storybook": "start-storybook -p 6006 -c ./config/storybook",
		"storybook:build": "build-storybook -c ./config/storybook",
		"prepare": "husky install",
		"postinstall": "node ./scripts/clearCache.js"
	},
